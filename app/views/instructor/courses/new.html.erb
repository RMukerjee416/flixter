<br />
<div class="booyah-box col-10 offset-1">
	<h1>Add a new course</h1>
	<%= simple_form_for @course, url: instructor_courses_path do |x| %>
		<%= x.input :title %>
		<%= x.input :description %>
		<%= x.input :cost %>
		<br />
		<%= x.submit "Create", class: 'btn btn-primary' %>
		<br />
	<% end %>
	<br />
</div>